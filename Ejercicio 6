void main() {
  List<int> numeros = [1, 2, 3, 4, 5];
  
  double promedio = calcularPromedio(numeros);
  
  print('El promedio de los n√∫meros es: $promedio');
}

double calcularPromedio(List<int> lista) {
  int suma = 0;

  for (int numero in lista) {
    suma += numero;
  }

  return suma / lista.length;
}
